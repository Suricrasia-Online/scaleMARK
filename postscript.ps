14 0 rmoveto
gsave
0 -14 rlineto
stroke
pop
grestore
} for
gsave
14 0 rmoveto
gsave
0 -40 rlineto
stroke
grestore
grestore
grestore
70 0 rmoveto
gsave
grestore
} for
} def


/f { findfont exch scalefont setfont } def

% 30 /FreeSans-Bold f
% 160 1850 moveto
% (SCALEMARK       SURICRASIA ONLINE       2019) show

/drawProtractor {
gsave
30 /FreeSans f
rotate
{
36 0 rmoveto
gsave
dup s
grestore
gsave
0 30 rmoveto
dup 180 exch sub s
grestore
gsave
0 -90 rmoveto
0 80 rlineto
stroke
grestore
gsave
0 60 rmoveto
0 70 rlineto
stroke
grestore
36 0 rmoveto
-10 rotate
} for
grestore
} def


/drawLogo {
gsave
85 /DejaVuSans f
/checkmark glyphshow
40 /FreeSans-Italic f
-25 10 rmoveto (scale) show
40 /FreeSans-Bold f
5 -1 rmoveto (MARK) show
18 /DejaVuSans f
0 20 rmoveto
/registered glyphshow
150 -15 rmoveto
24 /FreeSans-Bold f
(MADE IN TORONTO) show
grestore
} def


/drawScale {{
25 /FreeSans f
gsave

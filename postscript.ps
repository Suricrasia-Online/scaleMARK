/f { findfont exch scalefont setfont } def


/drawProtractor {
gsave
30 /FreeSans-Bold f
rotate
{
36 0 rmoveto
gsave
dup s
grestore
gsave
0 30 rmoveto
dup 180 exch sub s
grestore
gsave
0 -100 rmoveto
0 90 rlineto
stroke
grestore
gsave
0 60 rmoveto
0 100 rlineto
stroke
grestore
36 0 rmoveto
-10 rotate
} for
grestore
} def


/drawLogo {
gsave
85 /DejaVuSans f
/checkmark glyphshow
40 /FreeSans-Italic f
-25 10 rmoveto (scale) show
40 /FreeSans-Bold f
5 -1 rmoveto (MARK) show
18 /DejaVuSans f
0 20 rmoveto
/registered glyphshow
150 -15 rmoveto
24 /FreeSans-Bold f
(MADE IN TORONTO) show
grestore
} def


/drawScale {{
25 /FreeSans f
gsave
25 0 rmoveto
s
grestore
0 0 rmoveto
gsave
0 40 rmoveto
gsave
0 -40 rlineto
stroke
grestore
0 1 3 {
14 0 rmoveto
gsave
0 -14 rlineto
stroke
pop
grestore
} for
gsave
14 0 rmoveto
gsave
0 -40 rlineto
stroke
grestore
grestore
grestore
70 0 rmoveto
gsave
grestore
} for
} def

